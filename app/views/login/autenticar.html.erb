  <body>

<div class="container">

      <!-- Static navbar -->
      <!-- Main component for a primary marketing message or call to action -->
<center>      
<div class="jumbotron" >
 <p> <%= image_tag "logo2.png", width:220, height:100 %> </p> 

 <% flash.each do |type, message| %>
  <div class="alert <%= bootstrap_class_for(:error) %> fade in">
    <button class="close" data-dismiss="alert">×</button>
    <%= message %>
  </div>
 <% end %>

 <div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:login, url: login_path) do |f| %>
      
      <%= f.text_field :cedula, class: 'form-control', placeholder: "Cédula" %>

      <%= f.label :" " %>
      <%= f.password_field :clave, class: 'form-control', placeholder: "Clave" %>
      <hr>
      <%= f.submit "Ingresar", class: "btn btn-primary"  %>
    <% end %>   
  </div>
</center>

</div> <!-- /container -->


