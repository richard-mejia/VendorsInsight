    <div class="container">
<% provide(:title, "Inicio") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:login, url: login_path) do |f| %>

      <%= f.label :cedula %>
      <%= f.text_field :cedula, class: 'form-control' %>

      <%= f.label :clave %>
      <%= f.password_field :clave, class: 'form-control' %>

      <%= f.submit "Iniciar", class: "btn btn-primary" %>
    <% end %>

    
</div>
  </body>
</html>
